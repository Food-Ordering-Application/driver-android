<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_orders">

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.foa.driver.fragment.OrdersFragment"
        android:label="Đơn hàng"
        tools:layout="@layout/fragment_orders" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.foa.driver.fragment.WalletFragment"
        android:label="Ví"
        tools:layout="@layout/fragment_wallet" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.foa.driver.fragment.MapFragment"
        android:label="Bản đồ"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_statistic"
        android:name="com.foa.driver.fragment.StatisticFragment"
        android:label="Bản đồ"
        tools:layout="@layout/fragment_statistic" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.foa.driver.fragment.ProfileFragment"
        android:label="Thống kê"
        tools:layout="@layout/fragment_profile" />
</navigation>